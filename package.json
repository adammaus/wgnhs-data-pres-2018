{
  "name": "data-pres-2018",
  "private": true,
  "version": "1.1.3",
  "description": "",
  "scripts": {
    "build": "run-s build:clean build:prepare build:start",
    "build:clean": "rimraf dist",
    "build:prepare": "mkdirp dist",
    "build:start": "run-p build:rollup build:copy-resources",
    "build:rollup": "rollup -c --bundleConfigAsCjs",
    "build:copy-resources": "cpy \"*.html\" \".nojekyll\" \"404.html\" css/ dist/ --parents",
    "develop": "run-s build:copy-resources develop:watch",
    "develop:watch": "run-p develop:watch-build develop:watch-copy develop:serve",
    "develop:watch-build": "rollup -c -w --bundleConfigAsCjs",
    "develop:watch-copy": "onchange \"*.html\" \"css/*\" \".nojekyll\" \"404.html\" -- npx cpy {{changed}} dist/ --parents",
    "develop:serve": "live-server --no-css-inject --mount=/data-pres-2018/:dist/ --open=/data-pres-2018/ --middleware=\"../../dev/sghpa.js\" --wait=1000 dist/",
    "start": "run-s develop",
    "pages": "run-s build pages:origin",
    "pages:origin": "gh-pages -d dist",
    "pages:upstream": "gh-pages -d dist --remote upstream",
    "postversion": "run-s postversion:push-tags build pages:upstream",
    "postversion:push-tags": "git push upstream && git push --tags upstream",
    "release": "run-s release:install build release:version",
    "release:install": "npm ci",
    "release:version": "npm version patch"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "cpy-cli": "^3.1.1",
    "gh-pages": "^3.1.0",
    "live-server": "^1.2.1",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^4.0.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-minify-es": "^1.1.1",
    "rollup-plugin-node-resolve": "^5.0.1"
  },
  "dependencies": {}
}
